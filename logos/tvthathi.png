        '#bloco-cidade-4',
        '#bloco-cidade-5'
    ];

    var indiceAtual = 0;

    function mostrarBloco(indice) {

        $('.bloco-cidade-alternavel').removeClass('bloco-ativo');

        $(listaDeBlocos[indice]).addClass('bloco-ativo');
    }

    $('.seta-proxima').on('click', function() {
        indiceAtual++;
        if (indiceAtual >= listaDeBlocos.length) {
            indiceAtual = 0;
        }
        mostrarBloco(indiceAtual);
    });
    $('.seta-anterior').on('click', function() {
        indiceAtual--;
        if (indiceAtual < 0) {
            indiceAtual = listaDeBlocos.length - 1;
        }
        mostrarBloco(indiceAtual);
    });

    if (listaDeBlocos.length > 0) {
