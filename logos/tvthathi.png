        '#bloco-cidade-4',
        '#bloco-cidade-5'
    ];

    var indiceAtual = 0;

    function mostrarBloco(indice) {

        $('.bloco-cidade-alternavel').removeClass('bloco-ativo');

        $(listaDeBlocos[indice]).addClass('bloco-ativo');
    }

    $('.seta-proxima').on('click', function() {
        indiceAtual++;
        if (indiceAtual >= listaDeBlocos.length) {
            indiceAtual = 0;
        }
        mostrarBloco(indiceAtual);
    });
    $('.seta-anterior').on('click', function() {
        indiceAtual--;
        if (indiceAtual < 0) {
            indiceAtual = listaDeBlocos.length - 1;
        }
        mostrarBloco(indiceAtual);
    });

    if (listaDeBlocos.length > 0) {
        mostrarBloco(0);
    }
});</script></div></div><div class="td_block_wrap td_flex_block_3 tdi_340 td-pb-border-top th-play-center blocks-width td_block_template_1 td_flex_block"  data-td-block-uid="tdi_340" ><style>.tdi_340{margin-top:40px!important;padding-right:60px!important;padding-left:40px!important}@media (max-width:767px){.tdi_340{padding-right:40px!important;padding-left:40px!important}}@media (min-width:768px) and (max-width:1018px){.tdi_340{margin-right:30px!important;margin-left:30px!important}}</style><style>.tdi_340 .td_module_flex_1{width:50%;float:left}.rtl .tdi_340 .td_module_flex_1{float:right}.tdi_340 .td_module_column{width:50%;float:left}.tdi_340 .td_module_flex_3{width:100%;float:left;padding-bottom:13px;margin-bottom:13px}.tdi_340 .td_module_wrap{padding-left:24px;padding-right:24px}.tdi_340 .td_block_inner{margin-left:-24px;margin-right:-24px}.tdi_340 .td_module_flex_3 .td-module-container:before{bottom:-13px;border-width:0 0 1px 0;border-style:none;border-color:#eaeaea;border-color:#eaeaea}.tdi_340 .td_module_column:last-child .td_module_flex_3:last-child{margin-bottom:0!important;padding-bottom:0!important}.tdi_340 .td_module_flex_3:last-child .td-module-container:before{display:none}.tdi_340 .td_module_flex_3:nth-last-child(2){margin-bottom:13px!important;padding-bottom:13px!important}.tdi_340 .td_module_flex_3:nth-last-child(2) .td-module-container:before{display:block}.tdi_340 .td_module_flex_1 .td-module-container{border-color:#eaeaea;flex-direction:column}.tdi_340 .td_module_flex_3 .td-module-container{border-color:#eaeaea;align-items:initial}.tdi_340 .td_module_flex_1 .td-module-container:before{border-width:0 0 1px 0;border-style:none;border-color:#eaeaea;border-color:#eaeaea}.tdi_340 .td_module_flex_1 .entry-thumb{background-position:center 50%}.tdi_340 .td_module_flex_3 .entry-thumb{background-position:center 50%}.tdi_340 .td_module_flex_3 .td-image-container{flex:0 0 100%;width:100%}.tdi_340 .td_module_flex_1 .td-image-wrap{padding-bottom:100%}.ie10 .tdi